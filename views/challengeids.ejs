<div>
	<table border="1">
		<tr>
		<th>ChallengeID</th>	
		<th>Points</th>
		<th>Type</th>
		<th>Name</th>
		<th>Description</th>
		</tr>
		<% for(var i=0; i<rows.length; i++) {%>
			<tr>
			<td><%= rows[i].ChallengeID %></td>		
			<td><%= rows[i].Points %></td>
			<td><%= rows[i].Type %></td>
			<td><%= rows[i].Name %></td>
			<td><%= rows[i].Description %></td>
			</tr>
		<% } %>
	</table>
</div>
