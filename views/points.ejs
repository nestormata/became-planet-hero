<div>
	<table border="1">
		<tr>
			<th>UserID</th>
			<th>BadgeID</th>
			<th>Points</th>
			<th>Time</th>
		</tr>
		<% for(var i=0; i<rows.length; i++) {%>
			<tr>
				<td><%= rows[i].UserID %></td>
				<td><%= rows[i].BadgeID %></td>
				<td><%= rows[i].Points %></td>
				<td><%= rows[i].Time %></td>
			</tr>
		<% } %>
	</table>
</div>