<div>
	<table border="1">
		<tr>
		<th>TeamID</th>
		<th>TeamName</th>
		<th>TeamDescription</th>
		</tr>
		<% for(var i=0; i<rows.length; i++) {%>
			<tr>
			<td><%= rows[i].TeamID %></td>
			<tr><%= rows[i].TeamName %></tr>
			<tr><%= rows[i].TeamDescription %></tr>
			</tr>
		<% } %>
	</table>
</div>
